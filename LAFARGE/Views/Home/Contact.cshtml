    <label for="nomEntre" class="col-sm-2 col-form-label">NOM DE L'ENTREPRISE</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" id="nomEntre" value="Douar Laouzien Hay Saddina, Tetouan">
    </div>
  </div>
  <div class="form-group row" style="gap:unset;">
    <label for="ADRESSE" class="col-sm-2 col-form-label">ADRESSE</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" id="ADRESSE" value="LAFARGE CIMENTS TETOUAN">
